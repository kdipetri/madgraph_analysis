import model sm
define p = g u c d s u~ c~ d~ s~ b b~
generate p p > t t~ , ( t > w+ b, w+ > l+ vl ),( t~ > w- b~, w- > l- vl~) 
add process p p > t t~ j , ( t > w+ b, w+ > l+ vl) ,( t~ > w- b~, w- > l- vl~)
